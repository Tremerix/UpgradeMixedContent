{
	"manifest_version": 2,
	"name": "UpgradeMixedContent",
	"version": "0.1.3",
	"description": "Adds an 'upgrade-insecure-requests' CSP to all HTTPS server responses",
	"icons": {
		"48": "icons/icon.png"
	},
	"permissions": [
		"https://*/*", "webRequest", "webRequestBlocking"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"options_page": "options/options.html",
	"web_accessible_resources": [
        "options/options.js"
    ]
}
