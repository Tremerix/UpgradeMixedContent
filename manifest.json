{
	"manifest_version": 2,
	"name": "UpgradeMixedContent",
	"version": "0.1.5",
	"description": "Adds an 'upgrade-insecure-requests' CSP to all HTTPS server responses",
	"icons": {
		"48": "icons/icon.png"
	},
	"permissions": [
		"https://*/*", "webRequest", "webRequestBlocking"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"options_ui": {
		"page": "options/options.html"
	}
}
